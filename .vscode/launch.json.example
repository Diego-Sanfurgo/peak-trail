    "version": "0.2.0",
    "configurations": [
        {
            "name": "Zipnova choferes",
            "request": "launch",
            "type": "dart",
            "toolArgs": [
                "--dart-define=MAPBOX_TOKEN=your_MAPBOX_TOKEN",
            ]
        },
        {
            "name": "Zipnova choferes (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "toolArgs": [
                 "--dart-define=MAPBOX_TOKEN=your_MAPBOX_TOKEN",
            ]
        },
        {
            "name": "Zipnova choferes (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "toolArgs": [
                "--dart-define=MAPBOX_TOKEN=your_MAPBOX_TOKEN",
            ]
        },
        
    ]
